{
  "id": "1cf8a2d8",
  "name": "AIOTools",
  "version": "1.0.3",
  "main": "index.html",
  "manifestVersion": 5,
  "data": {
    "apiVersion": 2
  },
  "host": [
    {
      "app": "PS",
      "minVersion": "24.1"
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "fullAccess",
    "clipboard": "readAndWrite",
    "network": {
      "domains": "all"
    },
    "webview": {
      "allow": "yes",
      "domains": ["https://*.adobe.com", "https://*.google.com"]
    },
    "allowCodeGenerationFromStrings": true,
    "launchProcess": {
      "schemes": ["http", "https"],
      "extensions": [".svg", ".png"]
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "mainpanel",
      "label": {
        "default": "AIO Tools"
      },
      "minimumSize": {
        "width": 350,
        "height": 590
      },
      "maximumSize": {
        "width": 350,
        "height": 590
      },
      "preferredDockedSize": {
        "width": 350,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 350,
        "height": 590
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"]
        }
      ]
    },
    {
      "type": "panel",
      "id": "brushpanel",
      "label": {
        "default": "Brush Panel"
      },
      "minimumSize": { "width": 230, "height": 200 },
      "maximumSize": { "width": 350, "height": 300 },
      "preferredDockedSize": { "width": 230, "height": 300 },
      "preferredFloatingSize": { "width": 230, "height": 300 },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
      "species": ["pluginList"]
    }
  ]
}
